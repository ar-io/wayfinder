{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown"],
  "tsconfig": "tsconfig.json",
  "readme": "README.md",
  "name": "@ar.io/wayfinder-core API Reference",
  "includeVersion": true,
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "hideGenerator": true,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "categoryOrder": [
    "WayFinder",
    "Types",
    "Routing Strategies",
    "Gateway Providers",
    "Verification Strategies",
    "Utilities",
    "*"
  ],
  "exclude": ["**/*.test.ts", "**/*.spec.ts", "**/node_modules/**"],
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "treatWarningsAsErrors": false,
  "cleanOutputDir": true,
  "gitRevision": "develop",
  "sourceLinkTemplate": "https://github.com/ar-io/wayfinder/blob/{gitRevision}/packages/wayfinder-core/{path}#L{line}",
  "titleLink": "https://github.com/ar-io/wayfinder",
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "searchInDocuments": true
}
